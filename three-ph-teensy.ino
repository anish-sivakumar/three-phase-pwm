IntervalTimer pwmTimer;
volatile int index1 = 0;
volatile int index2 = 666;
volatile int index3 = 1333;

// for analog write, must be one of 3,4,5,6,9,10 and others for teensy
int PWM_out_pin_3 = 3; 
int PWM_out_pin_4 = 4; 
int PWM_out_pin_5 = 5; 
int PWM_out_pin_6 = 6; 
int PWM_out_pin_9 = 9; 
int PWM_out_pin_10 = 10; 

void updatePWM() {                 // interrupt service routine for overflow
   
  //phase A
  if (pwm1[index1] == 1) { //sint > Vc
    //turn on upper switch
    digitalWrite(PWM_out_pin_3, LOW); //lower IGBT
    digitalWrite(PWM_out_pin_4, HIGH); //upper IGBT
  } else {
    digitalWrite(PWM_out_pin_4, LOW); 
    digitalWrite(PWM_out_pin_3, HIGH);
  }
  
  //phase B
  if (pwm1[index2] == 1) { //sint > Vc
    //turn on upper switch
    digitalWrite(PWM_out_pin_5, LOW); //lower IGBT
    digitalWrite(PWM_out_pin_6, HIGH);//upper IGBT
  } else {
    digitalWrite(PWM_out_pin_6, LOW); 
    digitalWrite(PWM_out_pin_5, HIGH);
  }

  //phase C
  if (pwm1[index3] == 1) { //sint > Vc
    //turn on upper switch
    digitalWrite(PWM_out_pin_9, LOW); //lower IGBT
    digitalWrite(PWM_out_pin_10, HIGH);//upper IGBT
  } else {
    digitalWrite(PWM_out_pin_10, LOW); 
    digitalWrite(PWM_out_pin_9, HIGH);
  }
  index2++;
  index3++;
  index1++;
  
  if (index1 >= 1999) { 
    index1 = 0; 
  }
  if (index2 >= 1999) { 
    index2 = 0; 
  }
  if (index3 >= 1999) { 
    index3 = 0; 
  }

}


